{
	"name": "electron-md-to-pdf",
	"version": "1.1.0",
	"description": "Convert Markdown to PDF in Electron apps",
	"keywords": [
		"electron",
		"markdown",
		"md",
		"pdf",
		"convert",
		"export"
	],
	"main": "index.js",
	"author": "Samuel Meuli <me@samuelmeuli.com>",
	"repository": "github:samuelmeuli/electron-md-to-pdf",
	"license": "MIT",
	"scripts": {
		"start": "electron example/main.js",
		"format": "prettier --write",
		"lint": "eslint --fix",
		"release": "np"
	},
	"dependencies": {
		"showdown": "^1.9.0"
	},
	"peerDependencies": {
		"electron": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
	},
	"devDependencies": {
		"electron": "^5.0.1",
		"electron-debug": "^3.0.0",
		"eslint": "5.16.0",
		"eslint-config-airbnb-base": "13.1.0",
		"eslint-config-prettier": "^4.2.0",
		"eslint-plugin-import": "^2.17.2",
		"github-markdown-css": "^3.0.1",
		"husky": "^2.2.0",
		"lint-staged": "^8.1.6",
		"np": "^5.0.2",
		"prettier": "^1.17.0"
	},
	"files": [
		"index.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"yarn format",
			"yarn lint",
			"git add"
		],
		"*.{html,json,md,yml}": [
			"yarn format",
			"git add"
		]
	}
}
